{
  "name": "kintone-pretty-fields",
  "type": "module",
  "files": [
    "dist"
  ],
  "version": "0.10.6",
  "description": "Get kintone form fields in a pretty format.",
  "repository": {
    "type": "git",
    "url": "https://github.com/cy-takeuchi/kintone-pretty-fields.git"
  },
  "keywords": [
    "kintone"
  ],
  "author": "cy-takeuchi <cy.takeuchi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cy-takeuchi/kintone-pretty-fields/issues"
  },
  "homepage": "https://github.com/cy-takeuchi/kintone-pretty-fields",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/index.js",
        "require": "./dist/index.umd.cjs",
        "default": "./dist/index.js"
      },
      "browser": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc && vite build",
    "watch": "tsc && vite build --watch",
    "prepare": "npm run build",
    "test": "vitest",
    "check": "biome check --write ./src ./tests"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^6.0.0",
    "kintone-typeguard": "^0.17.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@types/node": "^24.10.1",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  }
}
